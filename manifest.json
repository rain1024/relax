{
  "name": "Relax",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Relax",
  "homepage_url": "http://github.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {},
  "options_page": "src/options/index.html",
  "permissions": [
    "tabs",
    "background"
  ]
}