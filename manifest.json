{
  "name": "Relax",
  "version": "1.0.0",
  "manifest_version": 1,
  "description": "Relax: You have 5 minutes to relax, just don't forget to breathe now",
  "homepage_url": "https://github.com/rain1024/relax",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {},
  "options_page": "src/options/index.html",
  "permissions": [
    "tabs",
    "background"
  ]
}